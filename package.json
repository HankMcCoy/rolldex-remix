{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "build": "npm run build:tailwind && remix build",
    "build:tailwind": "tailwindcss -o ./app/tailwind.css",
    "build:css": "postcss styles --base styles --dir app/styles --env production",
    "dev": "concurrently \"npm run dev:tailwind\" \"npm run dev:css\" \"remix dev\"",
    "dev:tailwind": "tailwindcss -o ./app/tailwind.css --watch",
    "dev:css": "postcss styles --base styles --dir app/styles -w",
    "postinstall": "remix setup node"
  },
  "dependencies": {
    "@heroicons/react": "^1.0.5",
    "@prisma/client": "^3.8.1",
    "@remix-run/react": "^1.0.6",
    "@remix-run/serve": "^1.0.6",
    "@remix-run/vercel": "^1.0.6",
    "@tailwindcss/forms": "^0.3.4",
    "@tailwindcss/typography": "^0.4.1",
    "@types/is-hotkey": "^0.1.7",
    "@types/react-modal": "^3.13.1",
    "concurrently": "^6.4.0",
    "is-hotkey": "^0.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-modal": "^3.14.4",
    "react-remarkable": "^1.1.3",
    "react-router-dom": "^6.2.1",
    "react-tether": "^2.0.8",
    "remix": "^1.0.6",
    "swr": "^1.2.2",
    "tailwindcss": "^2.2.19",
    "use-debounce": "^7.0.1",
    "utility-types": "^3.10.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.0.6",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "autoprefixer": "^10.4.2",
    "postcss": "^8.4.5",
    "postcss-cli": "^9.1.0",
    "postcss-nesting": "^10.1.1",
    "prisma": "^3.8.1",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
